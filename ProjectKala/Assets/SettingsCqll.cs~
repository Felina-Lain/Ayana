using UnityEngine;
using System.Collections;

public class SettingsCqll : MonoBehaviour {

	// Use this for initialization
	void Start () {
	
	}
	
	// Update is called once per frame
	public void CallSettingMenu (GameObject canvasOn) {

		canvasOn.gameObject.SetActive(true);	
	}

	public void CallSettingMenu (GameObject canvasOff) {

		canvasOff.gameObject.SetActive(false);	
	}
}
